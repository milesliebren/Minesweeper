const mongoose = require("mongoose");

// Define a schema
const Schema = mongoose.Schema;

const LeaderBoardEntryModelSchema = new Schema({
  username: 
    {
        type: String, 
        required: true,
        maxLength: 16
    },
  currentDate: 
    {
        type: Date, 
        required: true
    },
  elapsedTime: 
    {
        type: Number, 
        required: true
    },
});


module.exports = mongoose.model("Leaderboard_Entry", LeaderBoardEntryModelSchema);